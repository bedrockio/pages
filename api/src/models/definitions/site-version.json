{
  "attributes": {
    "name": {
      "type": "String",
      "required": true,
      "unique": true
    },
    "current": {
      "type": "Boolean",
      "default": false
    },
    "user": {
      "type": "ObjectId",
      "ref": "User",
      "required": true
    },
    "publishedAt": {
      "type": "Date",
      "default": "now"
    }
  }
}
